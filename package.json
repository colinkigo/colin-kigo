	"scripts": {
		"clean": "rimraf ./dist",
		"clean:config": "rimraf ./dist/configES2015.js",
		"clean:server": "rimraf ./dist/server/serverES2015.js",
		"move": "npm run move:config && npm run move:server",
		"move:config": "mv dist/config.js dist/configES2015.js",
		"move:server": "mv dist/server/server.js dist/server/serverES2015.js",
		"convert:es5": "npm run move && babel dist/server/serverES2015.js > dist/server/server.js && npm run clean:server",
		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "concurrently 'npm run client' 'npm run dev:ts'",
		"server": "ts-node ./src/server/server.ts",
		"dev:ts": "nodemon",
		"client": "webpack-dev-server --mode development --devtool inline-source-map",
		"build": "webpack --mode production",
		"start": "npm run build && node /dist/server/server.js"
	},
